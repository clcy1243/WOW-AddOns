<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
	<CheckButton name="ExtraButtonTemplate" inherits="AlternateButtonTemplate" virtual="true">
		<Attributes>
			<Attribute name="saveentries" value="ExtraBar_ButtonEntries"/>
			<Attribute name="savesettings" value="ExtraBar_ButtonSettings"/>
		</Attributes>
	</CheckButton>
	<Frame name="ExtraBarTemplate" inherits="SecureHandlerBaseTemplate, SecureHandlerShowHideTemplate" toplevel="true" frameStrata="LOW" virtual="true" hidden="false">
		<Size>
			<AbsDimension x="68" y="68"/>
		</Size>
		<Layers>
			<Layer level="ARTWORK">
				<FontString name="$parentNumber" inherits="GameFontGreenSmall" justifyH="LEFT" justifyV="TOP">
					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
				<FontString name="$parentConfigNumber" inherits="GameFontGreenSmall" justifyH="LEFT" justifyV="TOP">
					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
	</Frame>
	<Frame name="ExtraBar1" inherits="ExtraBarTemplate" parent="UIParent">
		<Anchors>
			<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT">
				<Offset>
					<AbsDimension x="-620" y="90"/>
				</Offset>
			</Anchor>
		</Anchors>
	</Frame>
		<Frame name="ExtraBar2" inherits="ExtraBarTemplate" parent="UIParent">
		<Anchors>
			<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT">
				<Offset>
					<AbsDimension x="-620" y="160"/>
				</Offset>
			</Anchor>
		</Anchors>
	</Frame>
		<Frame name="ExtraBar3" inherits="ExtraBarTemplate" parent="UIParent">
		<Anchors>
			<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT">
				<Offset>
					<AbsDimension x="-620" y="230"/>
				</Offset>
			</Anchor>
		</Anchors>
	</Frame>
		<Frame name="ExtraBar4" inherits="ExtraBarTemplate" parent="UIParent">
		<Anchors>
			<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT">
				<Offset>
					<AbsDimension x="-620" y="300"/>
				</Offset>
			</Anchor>
		</Anchors>
	</Frame>
</Ui>
